def checkNumber(arr, x):
    # Please add your code here
    l=len(arr)
    if l==0:
        return False
    if arr[0]==x:
        return True
    return checkNumber(arr[1:], x)

# Main
from sys import setrecursionlimit
setrecursionlimit(11000)
n=int(input())
arr=list(int(i) for i in input().strip().split(' '))
x=int(input())
if checkNumber(arr, x):
    print('true')
else:
    print('false')
